{
    "name": "alalm3i/laravel-cart",
    "description": "Laravel Shopping cart",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nabeel Alalmai",
            "email": "nabeelalalmai@gmail.com",
            "homepage": "https://nabeel.it/"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.18|^10.0",
        "illuminate/database": "^9.18|^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^7.7|^8.0",
        "pestphp/pest": "^1.22",
        "phpunit/phpunit": "^9.5.24",
        "laravel/pint": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Alalm3i\\LaravelCart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alalm3i\\LaravelCart\\Tests\\": "tests/"
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Alalm3i\\LaravelCart\\CartServiceProvider"
            ],
            "aliases": {
                "Cart": "Alalm3i\\LaravelCart\\Facades\\Cart"
            }
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
